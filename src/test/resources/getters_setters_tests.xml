<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Getters and Setters Tests">
    <test name="Man Tests">
        <classes>
            <class name="org.example.ManTests">
                <methods>
                    <include name="testGetFirstName" />
                    <include name="testGetLastName" />
                    <include name="testGetAge" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Woman Tests">
        <classes>
            <class name="org.example.WomanTests">
                <methods>
                    <include name="testGetFirstName" />
                    <include name="testGetLastName" />
                    <include name="testGetAge" />
                    <include name="testGetPartnerLastName" />
                </methods>
            </class>
        </classes>
    </test>
</suite>